{
  "name": "ts-bot-template",
  "version": "1.0.0",
  "description": "Simple discord bot template",
  "keywords": [],
  "author": {
    "name": "tovade",
    "url": "https://github.com/tovade"
  },
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,json,ts,tsx,yml,md}\"",
    "lint": "eslint . --ext .ts,.tsx",
    "test:tsc": "tsc --noEmit",
    "dev": "nodemon ./src/index.ts",
    "build": "tsc",
    "start": "node ./dist/src/index.js"
  },
  "devDependencies": {
    "@casper124578/eslint-config": "^4.3.0",
    "@types/glob": "^7.2.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.20.0",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4"
  },
  "prettier": {
    "semi": true,
    "trailingComma": "all",
    "singleQuote": false,
    "printWidth": 120,
    "tabWidth": 2
  },
  "dependencies": {
    "@discordjs/rest": "^1.0.1",
    "discord-api-types": "^0.37.0",
    "discord.js": "^14.1.2",
    "glob": "^8.0.3",
    "path": "^0.12.7"
  }
}
